<script>
    let oauthRef = null;
    function callOAuth(provider) {
        oauthRef = window.open(`/login/${provider}`, '_blank');
    }

    $(window).on("message", function(event) {
        if (
            !event ||
            !event.originalEvent ||
            !event.originalEvent.data ||
            event.originalEvent.data.type !== 'oauth-callback'
        ) return;
        
        oauthRef.close()

        const provider = event.originalEvent.data.value.provider;
        const response = event.originalEvent.data.value.response;

        if (event.originalEvent.data.value.isFailed) {
			// 팝업이 닫히고 alert이 뜰 수 있게끔 비동기화 한다.
			setTimeout(() => alert("오류가 발생하였습니다. 다시 시도해주세요."), 0);
			return;
		}

        fetch(`/api/v1/oauth/${provider}/id/${response.id}/exist`)
            .then(res => {
                if (res.ok) return res.json();

                throw res;
            })
            .then(alreadyExist => {
                const form = document.getElementById("oauth-login-form");
                form.user_id_of_provider.value = response.id;
                form.provider.value = provider;
                form.email.value = response.email;
                form.raw_data.value = JSON.stringify(response);

                if (!alreadyExist) {
                    form.action = '/accounts/membership'
                }

                form.submit();
            })
            .catch(_ => {
                alert("오류가 발생하였습니다. 다시 시도해주세요.");
            })
    });
</script>
                            </div>
        </div>
    </div>
        
