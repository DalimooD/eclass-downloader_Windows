    <div class="xn-change_language-select_dropdown">
        <button class="xn-change_language-select_dropdown-item" onclick="setLocale('ko')">
            한국어 (ko)
        </button>
        <button class="xn-change_language-select_dropdown-item" onclick="setLocale('en')">
            English (en)
        </button>
    </div>
