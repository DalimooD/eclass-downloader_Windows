<form action="/login/oauth" method="post" id="oauth-login-form">
    <input type="hidden" name="_token" value="sMCY8nwfhczXKzPmRqam484AGvLfMIK7QSipDtqo">
    <input type="hidden" name="user_id_of_provider" />
